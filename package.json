{
  "name": "test",
  "version": "1.0.0",
  "description": "ts config",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node --transpile-only src/index.ts",
    "prod": "npm run build && npm start",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/db.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/db.ts",
    "migration:revert": "npm run typeorm migration:revert -d src/config/db.ts"
  },
  "author": "koushik",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "typeorm": "^0.3.27"
  }
}
